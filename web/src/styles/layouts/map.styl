.l-map
  position absolute
  width 100%
  height 100%
  left 0
  top 0
  .c-map,
  .c-content
    position absolute
    width 100%
    height 100%
    left 0
    top 0
  .c-map
    z-index 0
  .c-content
    overflow-x hidden
    overflow-y scroll
    background-color white
    opacity 0.5
    z-index 100

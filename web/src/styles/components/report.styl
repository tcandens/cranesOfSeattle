.c-report
  display block
  width 100%
  height 100%
  $form-height = 100px
  &--forms
    height $form-height
    display flex
    background-color $dark
    flex-direction row
    align-content flex-start
    align-items flex-end
    justify-content space-between
  .mapbox-gl-container
    height: "calc(100vh - %s)" % ($form-height + $nav-height)
